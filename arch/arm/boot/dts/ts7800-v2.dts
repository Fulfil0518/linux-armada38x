//						gpio-hog;
//						gpios = <0x0 0x1>;
//						input;
//						line-name = "pcie1.0-clkreq";
//					};
//
//					pcie1_0_w_disable {
//						gpio-hog;
//						gpios = <0x3 0x1>;
